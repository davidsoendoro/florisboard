<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorWhite" />
            <size
                android:width="14dp"
                android:height="14dp" />
            <stroke
                android:width="1sp"
                android:color="#E0E0E0" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_cross_svg"
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp" />

</layer-list>
